{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/pages/index.js"
    },
    {
      "src": "/payment",
      "dest": "/pages/payment.js"
    },
    {
      "src": "/admin",
      "dest": "/pages/admin.js"
    },
    {
      "src": "/vietqr",
      "dest": "/pages/vietqr.js"
    }
  ]
}
